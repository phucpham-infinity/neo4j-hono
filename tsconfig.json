{
	"compilerOptions": {
		"target": "es2021" ,
		"lib": [
			"es2021"
		] ,
		"jsx": "react" ,
		"module": "es2022" ,
		"baseUrl": "./",                            
		"moduleResolution": "node" ,
		"types": [
			"@cloudflare/workers-types",
			"../../src"
		] ,                    
		"resolveJsonModule": true ,
		"allowJs": true ,
		"checkJs": false ,
		"noEmit": true,
		"isolatedModules": true ,
		"allowSyntheticDefaultImports": true ,
		"forceConsistentCasingInFileNames": true ,
		"strict": true,
		"skipLibCheck": true 
	}
}
